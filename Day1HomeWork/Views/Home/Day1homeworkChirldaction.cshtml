@model List<Day1HomeWork.Models.Day1ViewMode>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>


    <div class="row">
        <div class="col-md-12">
            <table class="table table-bordered table-hover">             
                <tr>
                    <th>#</th>
                    <th>類別</th>
                    <th>日期</th>
                    <th>金額</th>
                    <th>備註</th>
                </tr>
                
                @foreach (var Day1 in Model)
                {
                    <tr>
                        <td>@Day1.Dno</td>
                        <td>@Day1.Dtype</td>
                        <td>@Day1.Dday</td>
                        <td>@Day1.Dmoney</td>
                        <td>@Day1.Dnote</td>
                    </tr>
                }
                
            </table>
        </div>
    </div>
</body>
</html>
